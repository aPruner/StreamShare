<div class="container">
  <div class="imgshow">
    <%= image_tag(@image.filename.url, alt: 'Image', :class =>"imgshow")%>
  </div>
  <br>
  <%= link_to "Open", @image.filename_url,class: "btn btn-primary" %>
  <%= button_to "Delete",  @image, method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure that you wish to delete #{@image.title}?"} %>
</div>
